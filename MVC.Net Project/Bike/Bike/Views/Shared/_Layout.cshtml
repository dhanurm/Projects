<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link rel="stylesheet" type="text/css" href="~/Contentmaterialdesignicons.min.css" />
    <link rel="stylesheet" type="text/css" href="~/Content/jquery.mCustomScrollbar.min.css" />
    <link rel="stylesheet" type="text/css" href="~/Content/prettyPhoto.css" />
    <link rel="stylesheet" type="text/css" href="~/Content/template.css" />
    <link rel="stylesheet" type="text/css" href="~/Content/unslider.css" />

    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

    <style>
        body {
            background-color:cadetblue;
        }

        #header, #nav, .noprint {
            display: none;
        }
        
        .abc {
            float: left;
            width: 500px;
            margin: 5px;
        }
    </style>
</head>
<body data-target="#nino-navbar" data-spy="scroll">
    @RenderBody()
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
